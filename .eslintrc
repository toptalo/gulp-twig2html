{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "worker": true
  },
  "globals": {
    "caches": true,
    "Modernizr": true,
    "jQuery": true,
    "google": true,
    "ymaps": true,
    "YT": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-new": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-unused-vars": 0,
    "no-undef": 2,
    "no-unused-expressions": 2,
    "semi": [
      2,
      "always"
    ],
    "linebreak-style": 2,
    "dot-notation": 0
  }
}
